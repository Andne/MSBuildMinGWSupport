<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(VCTargetsPath)\Platforms\MinGW32\PlatformToolsets\MinGW\ImportBefore\*.props" Condition="Exists('$(VCTargetsPath)\Platforms\MinGW32\PlatformToolsets\MinGW\ImportBefore')" />

  <PropertyGroup>
    <IntDir Condition="'$(IntDir)'=='' AND '$(IntermediateOutputPath)'!=''">$(IntermediateOutputPath)</IntDir>
    <IntDir Condition="'$(IntDir)'=='' AND '$(IntermediateOutputPath)'==''">$(Configuration)\</IntDir>
    <OutDir Condition="'$(OutDir)'=='' AND '$(SolutionDir)' != ''">$(SolutionDir)$(Configuration)\</OutDir>
    <OutDir Condition="'$(OutDir)'=='' AND '$(SolutionDir)' == ''">$(IntDir)</OutDir>
  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\MinGW.Cpp.Common.props" />

  <PropertyGroup>
    <ExecutablePath Condition="'$(ExecutablePath)' == ''">C:\Tools\MinGW\bin;$(VSInstallDir)Common7\Tools\bin;$(VSInstallDir)Common7\tools;$(VSInstallDir)Common7\ide;$(ProgramFiles)\HTML Help Workshop;$(MSBuildToolsPath32);$(VSInstallDir);$(SystemRoot)\SysWow64;$(FxCopDir);$(PATH);</ExecutablePath>
    <IncludePath Condition="'$(IncludePath)' == ''">C:\Tools\MinGW\include;</IncludePath>
    <ReferencePath Condition="'$(ReferencePath)' == ''">C:\Tools\MinGW\lib;</ReferencePath>
    <LibraryPath Condition="'$(LibraryPath)' == ''">C:\Tools\MinGW\lib;</LibraryPath>
    <!-- <LibraryWPath Condition="'$(LibraryWPath)' == ''">$(WindowsSDK_MetadataPath)</LibraryWPath> -->
    <!-- <SourcePath Condition="'$(SourcePath)' == ''">$(VCInstallDir)atlmfc\src\mfc;$(VCInstallDir)atlmfc\src\mfcm;$(VCInstallDir)atlmfc\src\atl;$(VCInstallDir)crt\src;</SourcePath> -->
    <ExcludePath Condition="'$(ExcludePath)' == ''">C:\Tools\MinGW\include;$(MSBuildToolsPath32);C:\Tools\MinGW\lib;</ExcludePath>
    <NativeExecutablePath Condition="'$(NativeExecutablePath)' == ''">$(ExecutablePath)</NativeExecutablePath>
    <!-- <DebugCppRuntimeFilesPath Condition="'$(DebugCppRuntimeFilesPath)' == ''">$(VCInstallDir)redist\Debug_NonRedist\x86</DebugCppRuntimeFilesPath> -->
  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\Platforms\MinGW32\PlatformToolsets\MinGW\ImportAfter\*.props" Condition="Exists('$(VCTargetsPath)\Platforms\MinGW32\PlatformToolsets\MinGW\ImportAfter')" />
  
</Project>